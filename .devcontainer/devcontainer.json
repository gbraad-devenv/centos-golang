{
    "name": "CentOS Go development environment",
    "image": "ghcr.io/gbraad-devenv/centos-golang/code-cli:41",
    "runArgs": [ "--cap-add=NET_ADMIN", "--cap-add=NET_RAW", "--device=/dev/net/tun", "--device=/dev/fuse" ],
    "postStartCommand": ".github/install.sh",

    "overrideCommand": false,
	"privileged": true,
    "remoteUser": "gbraad",

    "customizations": {
        "vscode": {
            "extensions": [
                "ms-vscode.Theme-TomorrowKit",
            ]
        }
    }
}